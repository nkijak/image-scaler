{
    "name": "Image Stretch",
    "description": "Image Distortion Correction",
    "version": "2",
    "manifest_version": 2,
    "default_locale": "en",
    "permissions": [
        "fileBrowserHandler",
        {
            "fileSystem": [
                "write",
                "retainEntries",
                "directory"
            ]
        }
    ],
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "app": {
        "background": {
            "scripts": [
                "scripts/main.js",
                "scripts/chromereload.js"
            ]
        }
    }
}